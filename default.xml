<?xml version="1.0" encoding="UTF-8"?>
<manifest>

   <remote name="github"
           fetch="https://github.com/"/>

   <remote name="private"
   		   fetch="ssh://git@github.com" />

   <remote name="ibo-upstream"
 	   revision="husky-test"
	   sync-c="true"
	   sync-j="4"/>
	   
   <default revision="refs/heads/husky-robot-kinetic"
            remote="private"
            sync-c="true"
	        sync-j="4" />
	
	<project name="ibocerth/husky" path="src/husky" />
	<project name="ibocerth/velodyne_simulator" path="src/velodyne_simulator" />
	<project name="ibocerth/velodyne_driver" path="src/velodyne_driver" remote="ibo-upstream" />

	

</manifest>
