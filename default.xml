<?xml version="1.0" encoding="UTF-8"?>
<manifest>

   <remote name="github"
           fetch="https://github.com/"/>

   <remote name="private"
   		   fetch="ssh://git@github.com" />

   <remote name="uav"
	   revision="master" 
	   remote="private"
	   fetch="ssh://git@github.com"/>
	
   <remote name="extra_packages"
	   revision="thorvald-melodic-sim"
	   remote="private"
	   fetch="ssh://git@github.com"/>
	
   <remote name="extra_packages_uav"
	   revision="thorvald-melodic-sim-uav"
	   remote="private"
	   fetch="ssh://git@github.com"/>
	
   <default revision="refs/heads/master"
            remote="private"
            sync-c="true"
	        sync-j="4" />
	
  <!--uav-->
  <project path="src/ros_uav" name="iBOCerth/ros_uav" revision="melodic" remote="uav" />	
  <project path="px4-autopilot" name="iBOCerth/px4-autopilot" remote="extra_packages_uav"/>
  <!--ugv-->
  <project path="src/thorvald_melodic_simulation"  name="iBOCerth/thorvald_melodic_simulation" remote="extra_packages_uav" />
  <project path="src/navigation" name="iBOcerth/navigation" remote="extra_packages_uav"/>
  <project path="src/ros_path_planner" name="iBOcerth/ros_path_planner" remote="extra_packages_uav" />
  <project path="src/ros_gps_libs" name="iBOcerth/ros_gps_libs" remote="extra_packages" />
  <project path="src/ibo-scripts" name="iBOcerth/ibo-scripts" revision="thorvald-melodic-sim-uav">
	  <linkfile src="thorvald_exec.desktop" dest="thorvald_exec.desktop"/>
	  <linkfile src="killall.desktop" dest="killall.desktop"/>
	  <linkfile src="uav_gazebo.sh" dest="uav_gazebo.sh"/>
	  <linkfile src="uav_gazebo_headless.sh" dest="uav_gazebo_headless.sh"/>
	  <linkfile src="run.sh" dest="run.sh"/>
	  <linkfile src="bin/QGroundControl.AppImage" dest="QGroundControl.AppImage"/>
  </project>
	
  <project path="manifest" name="iBOCerth/manifest" remote="extra_packages_uav" />	
  <include name="snippets/upstream.xml" />
</manifest>
