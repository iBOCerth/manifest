<?xml version="1.0" encoding="UTF-8"?>
<manifest>

   <remote name="github"
           fetch="https://github.com/"/>

   <remote name="private"
   		   fetch="ssh://git@github.com" />
	   
   <default revision="refs/heads/husky-sim-melodic"
            remote="private"
            sync-c="true"
	        sync-j="4" />
	
	<project name="ibocerth/husky" path="src/husky">
		<copyfile src="killall.sh" dest="killall.sh" />
		<copyfile src="run_playpen.sh" dest="run_playpen.sh"/>
	</project>
	<project name="ibocerth/velodyne_simulator" path="src/velodyne_simulator" />
	<project name="ibocerth/velodyne_driver" path="src/velodyne_driver"/>
	<project name="ibocerth/eband_local_planner" path="src/eband_local_planner"/>
	<project name="ibocerth/gazebo_ros_pkgs" path="src/gazebo_ros_pkgs"/>

	<!--this manifest-->
	<project name="ibocerth/manifest" path="manifest"/>
 
	<!--gps lib-->
	<project name="ibocerth/ros_gps_libs" path="src/gps_libs" revision="master" />
 
</manifest>
